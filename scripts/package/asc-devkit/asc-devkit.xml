<?xml version="1.0" encoding="UTF-8"?>
<config name="asc-devkit" copy_type="delivery" src_path="lib/host" dst_path="usr/local/Ascend" install_path="asc-devkit"
        install_own="$username:$usergroup" install_mod="750" install_type="all">
    <package_info>
        <product_name>cann</product_name>
        <func_name>asc-devkit</func_name>
        <suffix>run</suffix>
        <install_script>scripts/package/asc-devkit/scripts/install.sh</install_script>
        <help>scripts/package/asc-devkit/scripts/help.info</help>
        <version>scripts/package/asc-devkit/version.xml</version>
        <check_features>true</check_features>
    </package_info>

    <generate_info value="scene.info" dst_path="asc-devkit" install_mod="640" feature="comm">
        <os>$(OS_NAME)</os>
        <os_version>$(OS_VER)</os_version>
        <arch>$(ARCH)</arch>
    </generate_info>

    <block_info dependtree="false" dst_path="asc-devkit" block_conf_path="ascend">
        <block name="EngineeringCommon"/>
        <block name="EngineeringFiles"/>
    </block_info>

    <dir_info value="asc-devkit" install_mod="550">
        <path value="$(TARGET_ENV)/tikcpp"/>
        <path value="$(TARGET_ENV)/tikcpp/tikcfw"/>
        <path value="$(TARGET_ENV)/tikcpp/tikcfw/include"/>
        <path value="$(TARGET_ENV)/tikcpp/ascendc_kernel_cmake"/>
        <path value="$(TARGET_ENV)/tikcpp/ascendc_kernel_cmake/modules"/>
        <path value="$(TARGET_ENV)/tikcpp/ascendc_kernel_cmake/modules/ascend_modules"/>
        <path value="$(TARGET_ENV)/tikcpp/ascendc_kernel_cmake/modules/ascend_modules/util"/>
        <path value="$(TARGET_ENV)/tikcpp/ascendc_kernel_cmake/modules/ascend_modules/util/makeself"/>
        <path value="$(TARGET_ENV)/ascendc"/>
        <path value="$(TARGET_ENV)/ascendc/include"/>
        <path value="$(TARGET_ENV)/ascendc/include/basic_api"/>
        <path value="$(TARGET_ENV)/ascendc/include/aicpu_api"/>
        <path value="$(TARGET_ENV)/ascendc/include/c_api"/>
        <path value="$(TARGET_ENV)/ascendc/include/highlevel_api"/>
        <path value="$(TARGET_ENV)/ascendc/include/highlevel_api/kernel_tiling"/>
        <path value="$(TARGET_ENV)/ascendc/include/highlevel_api/utils"/>
        <path value="$(TARGET_ENV)/ascendc/include/highlevel_api/utils/log"/>
        <path value="$(TARGET_ENV)/include/ascendc"/>
        <path value="$(TARGET_ENV)/include/ascendc/include"/>
        <path value="$(TARGET_ENV)/include/ascendc/highlevel_api"/>
        <path value="$(TARGET_ENV)/include/ascendc/basic_api"/>
        <path value="$(TARGET_ENV)/include/ascendc/host_api"/>
        <path value="$(TARGET_ENV)/include/ascendc/aicpu_api"/>
        <path value="$(TARGET_ENV)/include/ascendc/c_api"/>
        <path value="$(TARGET_ENV)/include/utils"/>
        <path value="$(TARGET_ENV)/include/utils/log"/>
        <path value="$(TARGET_ENV)/asc"/>
        <path value="$(TARGET_ENV)/asc/include"/>
        <path value="$(TARGET_ENV)/asc/include/tiling" install_softlink="$(TARGET_ENV)/ascendc/include/highlevel_api/tiling;$(TARGET_ENV)/include/tiling;$(TARGET_ENV)/include/ascendc/host_api/tiling"/>
        <path value="$(TARGET_ENV)/asc/include/adv_api" install_softlink="$(TARGET_ENV)/tikcpp/tikcfw/include/adv_api;$(TARGET_ENV)/include/ascendc/include/adv_api;$(TARGET_ENV)/tikcpp/tikcfw/lib;$(TARGET_ENV)/ascendc/include/highlevel_api/lib;$(TARGET_ENV)/include/ascendc/highlevel_api/lib"/>
        <path value="$(TARGET_ENV)/asc/impl"/>
        <path value="$(TARGET_ENV)/asc/impl/adv_api"/>
        <path value="$(TARGET_ENV)/lib64"/>
        <path value="$(TARGET_ENV)/lib64/device"/>
        <path value="$(TARGET_ENV)/lib64/device/lib64"/>
    </dir_info>
    <file_info copy_type="delivery" src_path="asc-devkit" dst_path="asc-devkit" install_path="asc-devkit" install_type="all">
        <file value="script" install_mod="550" entity="true"/>
        <file value="version.info" install_mod="550" entity="true"/>
        <file value="scene.info" install_mod="550" entity="true"/>
    </file_info>
    <file_info copy_type="delivery" src_path="asc-devkit/script" dst_path="asc-devkit/script" install_path="asc-devkit" install_type="all">
        <file value="filelist.csv" install_mod="550" entity="true"/>
    </file_info>

    <file_info copy_type="delivery" src_path="lib/host/asc/impl" dst_path="$(TARGET_ENV)/asc/impl" install_path="$(TARGET_ENV)/asc/impl" install_type="all">
        <file value="utils" install_mod="550" entity="true"/>
        <file value="basic_api" install_mod="550" entity="true"/>
        <file value="simt_api" install_mod="550" entity="true"/>
        <file value="micro_api" install_mod="550" entity="true"/>
        <file value="c_api" install_mod="550" entity="true"/>
    </file_info>
    <file_info copy_type="delivery" src_path="lib/host/asc/impl/adv_api" dst_path="$(TARGET_ENV)/asc/impl/adv_api" install_path="$(TARGET_ENV)/asc/impl/adv_api" install_type="all">
        <file value="detail" install_softlink="$(TARGET_ENV)/ascendc/include/highlevel_api/impl" install_mod="550" entity="true"/>
        <file value="tiling" install_mod="550" entity="true"/>
    </file_info>

    <file_info copy_type="delivery" src_path="lib/host/asc/include" dst_path="$(TARGET_ENV)/asc/include" install_path="$(TARGET_ENV)/asc/include" install_type="all">
        <file value="interface" install_softlink="$(TARGET_ENV)/ascendc/include/basic_api/interface;$(TARGET_ENV)/tikcpp/tikcfw/interface;$(TARGET_ENV)/include/ascendc/basic_api/interface" install_mod="550" entity="true"/>
        <file value="kernel_operator.h" install_softlink="$(TARGET_ENV)/ascendc/include/basic_api/kernel_operator.h;$(TARGET_ENV)/tikcpp/tikcfw/kernel_operator.h;$(TARGET_ENV)/include/ascendc/basic_api/kernel_operator.h" install_mod="550" entity="true"/>
        <file value="basic_api" install_mod="550" entity="true"/>
        <file value="simt_api" install_mod="550" entity="true"/>
        <file value="micro_api" install_mod="550" entity="true"/>
        <file value="c_api" install_mod="550" entity="true"/>
        <file value="tiling" install_mod="550"/>
        <file value="adv_api" install_mod="550" entity="true"/>
        <file value="utils" install_softlink="$(TARGET_ENV)/tikcpp/tikcfw/include/utils;$(TARGET_ENV)/include/ascendc/include/utils" install_mod="550" entity="true"/>
    </file_info>

    <file_info copy_type="delivery" src_path="lib/host/ascendc/include/highlevel_api" dst_path="$(TARGET_ENV)/ascendc/include/highlevel_api" install_path="$(TARGET_ENV)/ascendc/include/highlevel_api" install_type="all">
        <file value="kernel_tiling" install_softlink="$(TARGET_ENV)/tikcpp/tikcfw/kernel_tiling;$(TARGET_ENV)/include/ascendc/highlevel_api/kernel_tiling;$(TARGET_ENV)/include/kernel_tiling" install_mod="550" entity="true"/>
    </file_info>

    <file_info copy_type="delivery" src_path="lib/host/ascendc/include/basic_api" dst_path="$(TARGET_ENV)/ascendc/include/basic_api" install_path="$(TARGET_ENV)/ascendc/include/basic_api" install_type="all">
        <file value="impl" install_softlink="$(TARGET_ENV)/tikcpp/tikcfw/impl" install_mod="550" entity="true"/>
        <file value="op_frame" install_softlink="$(TARGET_ENV)/tikcpp/tikcfw/op_frame" install_mod="550" entity="true"/>
    </file_info>

    <file_info copy_type="delivery" src_path="lib/host/ascendc/include/aicpu_api" dst_path="$(TARGET_ENV)/ascendc/include/aicpu_api" install_path="$(TARGET_ENV)/ascendc/include/aicpu_api" install_type="all">
        <file value="aicpu_api.h" install_softlink="$(TARGET_ENV)/include/ascendc/aicpu_api/aicpu_api.h" install_mod="550" entity="true"/>
    </file_info>

    <file_info copy_type="delivery" src_path="lib/host/ascendc/include/highlevel_api/utils/log" dst_path="$(TARGET_ENV)/ascendc/include/highlevel_api/utils/log" install_path="$(TARGET_ENV)/ascendc/include/highlevel_api/utils/log" install_type="all">
        <file value="asc_cpu_log.h" install_softlink="$(TARGET_ENV)/include/utils/log/asc_cpu_log.h" install_mod="550" entity="true"/>
    </file_info>

    <file_info value="tikcpp_tiling_libs" copy_type="delivery" src_path="lib/host/tiling" dst_path="$(TARGET_ENV)/lib64" install_path="$(TARGET_ENV)/lib64" install_type="all" install_mod="440" feature="ascendc">
        <file value="libtiling_api.a" file_type="static" pkg_feature="filter_as31xm1_device"/>
    </file_info>

    <file_info value="acl_rtc" copy_type="delivery" src_path="lib/host" dst_path="$(TARGET_ENV)" install_path="$(TARGET_ENV)/lib64" install_type="all" install_mod="440">
        <file value="libacl_rtc.so" file_type="shared"/>
    </file_info>

    <file_info copy_type="delivery" src_path="lib/device" dst_path="$(TARGET_ENV)" install_path="$(TARGET_ENV)/lib64/device/lib64" pkg_feature="filter_florence">
        <file value="libaicpu_api.a" file_type="static"/>
    </file_info>

    <file_info value="op_build" copy_type="delivery" src_path="lib/host" dst_path="$(TARGET_ENV)/../lib" install_path="tools/opbuild" install_mod="550">
        <file value="op_build" file_type="executable"/>
    </file_info>

    <file_info value="ascendc_scripts" copy_type="delivery" src_path="lib/host" dst_path="$(TARGET_ENV)" install_path="tools/ascendc_tools" install_mod="550" feature="ascendc">
        <file value="ascendc_parse_dumpinfo.py"/>
    </file_info>

    <file_info copy_type="source" src_path="build/makeself" dst_path="makeself" install_path="$(TARGET_ENV)/tikcpp/ascendc_kernel_cmake/modules/ascend_modules/util/makeself">
        <file value="COPYING" install_mod="550"/>
        <file value="make-release.sh" install_mod="550"/>
        <file value="Makefile" install_mod="550"/>
        <file value="makeself-header.sh" install_mod="550"/>
        <file value="makeself.1" install_mod="550"/>
        <file value="makeself.sh" install_mod="550"/>
        <file value="README.md" install_mod="550"/>
        <file value="VERSION" install_mod="550"/>
    </file_info>

    <file_info copy_type="delivery" src_path="lib/host" dst_path="$(TARGET_ENV)" install_path="$(TARGET_ENV)/tikcpp">
        <file value="ascendc_kernel_cmake" entity="true"/>
    </file_info>

</config>

