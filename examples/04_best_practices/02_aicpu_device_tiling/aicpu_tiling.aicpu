#include "aicpu_api.h"
#include "kernel_args.h"

__global__ __aicpu__ uint32_t MyAicpuKernel(KernelInfo::KernelArgs args)
{
    AscendC::printf("MyAicpuKernel inited\n");
    args.ti->type = 1;
    args.ti->mode = 2;
    args.ti->len = 4;
    AscendC::DataStoreBarrier();
    AscendC::printf("MyAicpuKernel inited type %u mode %u len %u end!\n", args.ti->type, args.ti->mode, args.ti->len);
    return 0;
}
