@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/@STATIC_TARGET_NAME@-targets.cmake")

set(@STATIC_TARGET_NAME@_DEP_LIB_DIR "$ENV{ASCEND_HOME_PATH}"
    CACHE PATH "依赖库的链接目录（来自环境变量）")

if(NOT @STATIC_TARGET_NAME@_DEP_LIB_DIR)
    message(WARNING "环境变量 MYLIB_DEP_LIB_DIR 未设置，可能导致链接失败")
endif()

target_link_directories(@STATIC_TARGET_NAME@::@STATIC_TARGET_NAME@
    INTERFACE ${@STATIC_TARGET_NAME@_DEP_LIB_DIR})

set_target_properties(@STATIC_TARGET_NAME@::@STATIC_TARGET_NAME@ PROPERTIES
  INTERFACE_LINK_DIRECTORIES "${@STATIC_TARGET_NAME@_DEP_LIB_DIR}/lib64")
